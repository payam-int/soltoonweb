{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": 744,
  "autoInstall": false,
  "rtl": true,
  "components": [
    "reset",
    "button",
    "flag",
    "image",
    "input",
    "label",
    "form",
    "message",
    "table",
    "icon",
    "dropdown",
    "transition",
    "progress",
    "form",
    "visibility"
  ],
  "version": "2.2.13"
}